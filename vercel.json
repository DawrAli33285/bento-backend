{
    "version":2,
    "builds":[
        {
            "src":"*.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "build": {
        "env": {
          "NODE_ENV": "production"
        },
        "commands": [
          "npx playwright install",
          "npm run build"
        ]
      }
    
}